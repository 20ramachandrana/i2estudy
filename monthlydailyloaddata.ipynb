    "# Setup for the module\n",
    "import csv\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import time\n",
    "from datetime import date, datetime\n",
    "\n",
    "fn = '\Apartments\Apt99_2015.csv'"
   ]
